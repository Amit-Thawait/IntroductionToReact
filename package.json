    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "babel-core": "^6.3.13",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.0",
    "history": "^1.14.0",
    "jest-cli": "^0.8.1",
    "jquery": "^2.1.4",
    "react-addons-test-utils": "^0.14.3",
    "style-loader": "^0.13.0",
